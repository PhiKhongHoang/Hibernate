package model;

import java.sql.Date;

public class Cat {
	private Integer id;
	private String name;
	private Date dob;
	private boolean sex;
	
//	bắt buộc có 1 constructor rỗng
	public Cat() {}

	public Cat(String name, Date dob, boolean sex) {
		this.name = name;
		this.dob = dob;
		this.sex = sex;
	}
	
	public Cat(Integer id, String name, Date dob, boolean sex) {
		this.id = id;
		this.name = name;
		this.dob = dob;
		this.sex = sex;
	}
	
}
